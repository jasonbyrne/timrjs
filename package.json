{
  "name": "timrjs",
  "version": "0.7.7",
  "description": "Timr is a simple, event driven utility for creating timers in JavaScript.",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --timeout 5000 --compilers js:babel-register",
    "test:watch": "npm test -- --watch",
    "clean": "rimraf dist/ && rimraf lib/",
    "lint": "gulp lint",
    "build": "NODE_ENV=production gulp",
    "predeploy": "npm run clean && npm run build",
    "deploy:github": "git push origin master",
    "deploy:npm": "npm publish",
    "deploy": "npm run deploy:npm && npm run deploy:github"
  },
  "keywords": [
    "timr",
    "timer",
    "timrjs",
    "utility",
    "countdown",
    "stopwatch"
  ],
  "bugs": {
    "url": "https://github.com/joesmith100/timrjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joesmith100/timrjs.git"
  },
  "author": "Joe Smith <joesmith0488@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-plugin-import": "^1.10.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.4",
    "gulp-util": "^3.0.7",
    "gulp-wrap": "^0.13.0",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.3",
    "through2": "^2.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "object-assign": "^4.1.0"
  }
}
